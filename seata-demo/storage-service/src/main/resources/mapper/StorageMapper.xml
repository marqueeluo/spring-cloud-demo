<mapper namespace="com.luo.demo.seata.mapper.StorageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.luo.demo.seata.model.entity.Storage">
        <id column="id" property="id" />
